{
  "extends": ["react-app", "plugin:jsx-a11y/recommended", "eslint:recommended"],
  "plugins": ["jsx-a11y"],
  "rules": {
    "no-console": "warn",
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "jsx-a11y/no-autofocus": 1,
    "jsx-a11y/label-has-for": 0,
    "jsx-a11y/label-has-associated-control": 0
  }
}
